  "scripts": {
    "installer": "npm i --global yarn@latest && npx yarn install --audit && yarn audit-fix",
    "audit-fix": "npx yarn-audit-fix",
    "update": "npx yarn upgrade-interactive --latest",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx",
    "codegen": "graphql-codegen --resolve-plugins-relative-to . --config codegen.yml",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "cypress": "cypress open",
    "cypress:ci": "cypress run",
    "e2e": "start-server-and-test start http://localhost:3000 cypress",
    "e2e:ci": "start-server-and-test start http://localhost:3000 cypress:ci"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
